/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
	Nand (a=a, b=b, out=q);
	Nand (a=a, b=q, out=w);
	Nand (a=b, b=q, out=e);
	Nand (a=w, b=e, out=out);
}
