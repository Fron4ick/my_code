/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Nand (a=a, b=sel, out=q);
	Nand (a=q, b=a, out=w);
	Nand (a=b, b=sel, out=e);
	Nand (a=w, b=e, out=out);
}
